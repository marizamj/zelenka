{
  "name": "zelenka",
  "version": "0.1.0",
  "private": true,
  "homepage": "http://zelenka.online/",
  "dependencies": {
    "gh-pages": "^2.0.1",
    "react": "^16.5.2",
    "react-collapse": "^4.0.3",
    "react-dom": "^16.5.2",
    "react-motion": "^0.5.2",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.0.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "clean": "rm -rf ./build",
    "build": "npm run clean && react-scripts build",
